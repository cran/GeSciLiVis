Type: Package
Package: GeSciLiVis
Title: Gene-Based Publication Activity Visualiser
Version: 1.1.1
Authors@R: c(
      person("Marina","Leer",email="marina.leer@dife.de",role = c("aut", "cre","cph")),
      person("George", "Soultoukis", role = c("ctb")),
